{
  "name": "@folkdb/editor",
  "version": "0.0.0",
  "description": "Editor app for CRUD operations on FolkDB",
  "author": "Daniel C. Narey",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/folkdb/editor"
  },
  "type": "module",
  "scripts": {
    "build": "del dist/* && svelte-render",
    "build:dev": "del dist/* && svelte-render --development",
    "merge:next": "git checkout master && git pull && git merge next -s recursive -Xtheirs",
    "serve": "sirv start dist --port=8080",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "npm run build",
    "update": "updtr"
  },
  "dependencies": {
    "@metamodern/copy-typefaces": "^2.0.3",
    "@metamodern/design-system": "^2.0.0",
    "@metamodern/pug-filter-markdown": "^1.0.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "cssnano": "^4.1.10",
    "graphql-request": "^3.0.0",
    "navaid": "^1.1.1",
    "netlify-identity-widget": "^1.6.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sugarss": "^2.0.0",
    "svelte-preprocess": "^3.7.4",
    "tailwindcss": "^1.4.6",
    "typeface-barlow": "0.0.71",
    "typeface-barlow-semi-condensed": "0.0.71",
    "typeface-source-code-pro": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@metamodern/svelte-render": "^2.0.0",
    "core-js": "^3.6.5",
    "del-cli": "^3.0.0",
    "postcss": "^7.0.32",
    "pug": "^3.0.0",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.23.0",
    "updtr": "^3.1.0"
  },
  "engines": {
    "node": ">=12"
  }
}
